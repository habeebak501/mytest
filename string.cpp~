*/#include <iostream>
#include <string>
#include<stdio.h>
using namespace std;
int main(){
    string str,str1;
int index = 0;
cout<< "Enter a string"<<endl;
 //scanf("%10[0-9a-zA-Z ]", str);
//scanf(" %[^\n]s",str);
 getline(cin,str);

 int i,j;
 for(i=0;str[i]!='\0';i++)
    {
    if(str[i]==' ')
    {
        for(j=i;str[j]!='\0';j++)
         {
            str[j]=str[j+1];
         }
        str[j]='\0';
    }
 }
    cout<<str<<endl;



return 0;
}
